{
  "java.jdt.ls.java.home": "/home/bbont/wpilib/2025/jdk",
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "extensions.ignoreRecommendations": true,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "java.completion.matchCase": "off",
  "terminal.integrated.env.linux": {
    "JAVA_HOME": "/home/bbont/wpilib/2025/jdk",
    "PATH": "/home/bbont/wpilib/2025/jdk/bin:${env:PATH}"
  },
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-17",
      "path": "/home/bbont/wpilib/2025/jdk",
      "default": true
    }
  ],
  "redhat.telemetry.enabled": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.inlineSuggest.syntaxHighlightingEnabled": true,
  "outline.collapseItems": "alwaysCollapse",
  "editor.formatOnPaste": true,
  "diffEditor.codeLens": true,
  // Place your settings in this file to overwrite the default settings
  //-------- Editor configuration --------
  // Controls the font family.
  "editor.fontFamily": "DejaVu Sans Mono",
  "editor.fontWeight": "normal",
  // Controls visibility of the glyph margin
  "editor.glyphMargin": true,
  // Controls whether the editor should render whitespace characters
  "editor.renderWhitespace": "all",
  // Controls the rendering size of tabs in characters. Accepted values: "auto", 2, 4, 6, etc. If set to "auto", the value will be guessed when a file is opened.
  "editor.tabSize": 4,
  "editor.rulers": [
    80,
    120
  ],
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.autoIndent": "advanced",
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "block-outline",
  "editor.fontLigatures": true,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.stablePeek": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "onlySnippets",
  //-------- Files configuration --------
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.gs": "javascript"
  },
  "autoDocstring.docstringFormat": "sphinx",
  "autoDocstring.generateDocstringOnEnter": false,
  "autoDocstring.includeName": true,
  "autoDocstring.startOnNewLine": true,
  "breadcrumbs.enabled": true,
  "css.lint.duplicateProperties": "warning",
  "css.lint.float": "warning",
  "css.lint.important": "warning",
  "debug.inlineValues": true,
  "debug.internalConsoleOptions": "neverOpen",
  "debug.openDebug": "neverOpen",
  "debug.showInStatusBar": "always",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.incrementalNaming": "smart",
  "git.autofetch": true,
  "jupyter.disableJupyterAutoStart": true,
  "jupyter.exportWithOutputEnabled": true,
  "mssql.connections": [],
  "mssql.format.alignColumnDefinitionsInColumns": true,
  "mssql.format.datatypeCasing": "uppercase",
  "mssql.format.keywordCasing": "uppercase",
  "mssql.format.placeCommasBeforeNextStatement": true,
  "mssql.format.placeSelectStatementReferencesOnNewLine": true,
  "notebook.lineNumbers": "on",
  "python.languageServer": "Default",
  "search.smartCase": true,
  "sql-formatter.uppercase": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": 400,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.commandCenter": false,
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.splitInGroupLayout": "vertical",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "workbench.list.horizontalScrolling": true,
  "workbench.list.openMode": "doubleClick",
  "workbench.startupEditor": "newUntitledFile",
  //==== EMMET PREFERENCES ====
  "emmet.preferences": {
    "filter.commentAfter": " <!-- /[#ID][.CLASS] -->",
  },
  "emmet.syntaxProfiles": {
    "html": {
      "filters": "c"
    },
  },
  "todo-tree.highlights.enabled": true,
  "todo-tree.general.revealBehaviour": "start of todo",
  "todo-tree.general.statusBar": "tags",
  "todo-tree.general.tags": [
    "TODO",
    "FIXME",
    "REVIEW",
    "NOTE",
    "HELP",
    "MERGE CONFLICT"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "*.jsonc"
      ],
      "schema": {
        "allowTrailingCommas": true
      }
    }
  ],
  "markdown.extension.list.indentationSize": "inherit",
  "markdown.extension.print.absoluteImgPath": false,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.extension.toc.levels": "1..3",
  "markdown.extension.toc.slugifyMode": "vscode",
  "markdownlint.config": {
    "default": true,
    "MD007": {
      "indent": 4
    }
  },
  "[markdown]": {
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.unicodeHighlight.ambiguousCharacters": true,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
  },
  "[powershell]": {
    "editor.tabSize": 4,
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.wordBasedSuggestions": "matchingDocuments",
    "editor.formatOnType": true
  },
  "[yml]": {
    "editor.tabSize": 4,
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[sql]": {
    "editor.defaultFormatter": "ms-mssql.mssql"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "dotnetAcquisitionExtension.existingDotnetPath": [],
  "workbench.editor.empty.hint": "hidden",
  "explorer.openEditors.sortOrder": "alphabetical",
  "diffEditor.experimental.showMoves": true,
  "editor.inlineSuggest.edits.showCollapsed": true,
  "terminal.integrated.middleClickBehavior": "paste",
  "terminal.integrated.tabStopWidth": 4,
  "editor.minimap.autohide": true,
  "editor.minimap.scale": 2,
  "editor.minimap.size": "fit",
  "editor.detectIndentation": false,
}

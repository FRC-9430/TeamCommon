	"Print Line": {
		"prefix": "println",
		"body": [
			"System.out.println(\"$1\");",
		],
		"description": "Prepare a System.out.println call"
	},

	"Javadoc Block": {
		"prefix": "javadoc",
		"body": [
			"/**",
			" * ${1:Brief description of the method or class.}",
			" * ",
			" * @author Your Full Name",
			" * ${2: * @param ${3:param_name} description of the parameter.}",
			" * ${4: * @return ${5:return_type} description of the return value.}",
			" */"
		],
		"description": "A Javadoc block template for documenting methods, classes, and other structures. Manually fill in parameters and returns as needed."
	}
